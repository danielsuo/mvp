@import url('./svg.select.css');

panelWidth = 20%
appWidth = 100% - panelWidth

body
  margin 0

#panel
  position absolute
  height 100%
  width panelWidth
  background-color #f0f

#info
  position absolute
  height 20%
  width 100%
  left 0
  bottom 0
  background-color #ff0
  
#protofit
  position absolute
  height 100%
  width appWidth
  right 0